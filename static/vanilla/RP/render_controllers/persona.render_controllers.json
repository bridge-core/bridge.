  "render_controllers": {
    "controller.render.persona_animated_face.first_person": {
      "rebuild_animation_matrices": true,
      "geometry": "Geometry.animated_face",
      "materials": [ { "*": "Material.animated" } ],
      "textures": [ "Texture.animated_face" ],
      "part_visibility": [
        { "*": false },
        { "rightArm": "query.get_rendered_item_name == '' || query.get_rendered_item_name == 'map'" },
        { "rightSleeve": "query.get_rendered_item_name == '' || query.get_rendered_item_name == 'map'" },
        { "leftArm": "(query.get_rendered_item_name == 'map' && query.get_equipped_item_name('off_hand') != 'shield') || (query.get_equipped_item_name('off_hand') == 'map' && !query.item_is_charged) || (!query.item_is_charged && (variable.item_use_normalized > 0 && variable.item_use_normalized < 1.0))" },
        { "leftSleeve": "(query.get_rendered_item_name == 'map' && query.get_equipped_item_name('off_hand') != 'shield') || (query.get_equipped_item_name('off_hand') == 'map' && !query.item_is_charged) || (!query.item_is_charged && (variable.item_use_normalized > 0 && variable.item_use_normalized < 1.0))" }
      ],
      "uv_anim": {
        "offset": [ 0.0, "variable.is_blinking * 0.5" ],
        "scale": [ 1.0, 1.0 ]
      }
    },
