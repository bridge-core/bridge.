{
  "format_version": "1.10.0",
  "particle_effect": {
    "description": {
      "identifier": "minecraft:water_drip_particle",
      "basic_render_parameters": {
        "material": "particles_alpha",
        "texture": "textures/particle/particles"
      }
    },
    "events": {
      "splash": {
        "particle_effect": {
          "effect": "minecraft:rain_splash_particle",
          "type": "particle"
        }
      }
    },
