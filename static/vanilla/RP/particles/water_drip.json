        "uv": {
          "texture_width": 128,
          "texture_height": 128,
          "uv": [ 8, 56 ],
          "uv_size": [ 8, 8 ]
        }
      },
      "minecraft:particle_appearance_tinting": {
        "color": [ 0.2, 0.3, 1.0 ]
      },
