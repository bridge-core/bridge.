{
	"format_version" : "1.10.0",
	"animation_controllers" : {
		"controller.animation.parrot.move" : {
			"initial_state" : "dancing",
			"states" : {
				"dancing" : {
					"animations" : [ "dance" ],
					"transitions" : [
						{
							"flying" : "variable.state == 0"
						},
						{
							"standing" : "variable.state == 1"
						},
						{
							"sitting" : "variable.state == 2"
						}
					]
				},
				"flying" : {
					"animations" : [ "moving", "flying" ],
					"transitions" : [
						{
							"standing" : "variable.state == 1"
						},
						{
							"sitting" : "variable.state == 2"
						},
						{
							"dancing" : "variable.state == 3"
						}
					]
				},
				"sitting" : {
					"animations" : [ "sitting" ],
					"transitions" : [
						{
							"flying" : "variable.state == 0"
						},
						{
							"standing" : "variable.state == 1"
						},
						{
							"dancing" : "variable.state == 3"
						}
					]
				},
				"standing" : {
					"animations" : [ "moving", "standing" ],
					"transitions" : [
						{
							"flying" : "variable.state == 0"
						},
						{
							"sitting" : "variable.state == 2"
						},
						{
							"dancing" : "variable.state == 3"
						}
					]
				}
			}
		},
		"controller.animation.parrot.setup" : {
			"initial_state" : "default",
			"states" : {
				"default" : {
					"animations" : [ "look_at_target", "base" ]
				}
			}
		}
	}
}
