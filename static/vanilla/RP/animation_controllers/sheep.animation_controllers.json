{
	"format_version" : "1.10.0",
	"animation_controllers" : {
		"controller.animation.sheep.move" : {
			"initial_state" : "default",
			"states" : {
				"default" : {
					"animations" : [
						{
							"walk" : "query.modified_move_speed"
						}
					],
					"transitions" : [
						{
							"grazing" : "query.is_grazing"
						}
					]
				},
				"grazing" : {
					"animations" : [ "grazing" ],
					"transitions" : [
						{
							"default" : "query.all_animations_finished"
						}
					]
				}
			}
		}
	}
}
