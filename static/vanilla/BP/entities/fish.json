        "is_pushable": true,
        "is_pushable_by_piston": true
      },
      "minecraft:movement.sway": {
        "sway_amplitude": 0.0
      },
      "minecraft:behavior.swim_idle": {
        "priority": 5
      },
      "minecraft:behavior.random_swim": { // Randomly selects points to go to, even in water
        "priority": 3,
        "speed_multiplier": 1.0,
        "xz_dist": 16,
        "y_dist": 4,
        "interval": 0
