{
    "@meta": {
        "expand_path_exceptions": [
            "minecraft:bucket",
            "minecraft:bed",
            "minecraft:coal",
            "minecraft:dye",
            "minecraft:fire_charge",
            "minecraft:fishing_rod",
            "minecraft:flint_and_steel",
            "minecraft:bottle",
            "minecraft:lead",
            "minecraft:name_tag",
            "saddle",
            "minecraft:shears",
            "minecarft:skull",
            "minecraft:mob_placer"
        ]
    },
    "minecraft:use_duration": "$general.number",
    "minecraft:max_stack_size": "$general.number",
    "minecraft:max_damage": "$general.number",
    "minecraft:hand_equipped": "$general.boolean",
    "minecraft:stacked_by_data": "$general.boolean",
    "minecraft:foil": "$general.boolean",
    "minecraft:bucket": {},
    "minecraft:bed": {},
    "minecraft:frame_count": "$general.number",
    "minecraft:animates_in_toolbar": "$general.boolean",
    "minecraft:ignores_permission": "$general.boolean",
    "minecraft:coal": {},
    "minecraft:mining_speed": "$general.number",
    "minecraft:damage": "$general.number",
    "minecraft:can_destroy_in_creative": "$general.boolean",
    "minecraft:dye": {},
    "minecraft:requires_interact": "$general.boolean",
    "minecraft:fire_charge": {},
    "minecraft:mirrored_art": "$general.boolean",
    "minecraft:fishing_rod": {},
    "minecraft:flint_and_steel": {},
    "minecraft:bottle": {},
    "minecraft:lead": {},
    "minecraft:name_tag": {},
    "minecraft:explodable": "$general.boolean",
    "minecraft:should_despawn": "$general.boolean",
    "saddle": {},
    "minecraft:shears": {},
    "minecarft:skull": {},
    "minecraft:mob_placer": {},

    "minecraft:door": {
        "material": ["metal", "wood"],
        "type": "$_general.block_states.wood_type and iron"
    },
    "minecraft:sign": {
        "type": "$_general.block_states.wood_type"
    },
    "minecraft:allow_off_hand": "$general.boolean",
    "minecraft:projectile": {
        "projectile_entity": "$general.entity_identifier",
        "minimum_critical_power": "$general.decimal"
    },
    "minecraft:block_placer": {
        "block": "$general.block_identifier",
        "use_block_descritpion": "$general.boolean"
    },
    "minecraft:liquid_clipped": "$general.boolean",
    "minecraft:entity_placer": {
        "entity": "$general.entity_identifier",
        "allowed_blocks": {
            "$dynamic.list.next_index": "$general.block_identifier"
        }
    },
    "minecraft:enchantable": {
        "slot": "$item.general.slots",
        "value": "$general.number"
    },
    "minecraft:shooter": {
        "max_draw_duration": "$general.number",
        "charge_on_draw": "$general.boolean",
        "scale_power_by_draw_duration": "$general.boolean",
        "ammunition": {
            "$dynamic.list.next_index": {
                "item": "$general.item_identifier",
                "use_offhand": "$general.boolean",
                "search_inventory": "$general.boolean",
                "use_in_creative": "$general.boolean"
            }
        }
    },
    "minecraft:damageable": {
        "max_damage": "$general.number",
        "damage_chance": {
            "min": "$general.number",
            "max": "$general.number"
        },
        "repair_items": {
            "$dynamic.list.next_index": "$general.item_identifier"
        }
    },
    "minecraft:armor": {
        "protection": "$general.number"
    },
    "minecraft:wearable": {
        "slot": "$general.slot_type"
    },
    "minecraft:cooldown": {
        "category": "$item.general.cooldown_types"
    },
    "minecraft:use_on": {
        "blocks": {
            "$dynamic.list.next_index": "$general.block_identifier"
        }
    },
    "minecraft:digger": {
        "use_efficiency": "$general.boolean",
        "destroy_speeds": {
            "$dynamic.list.next_index": {
                "block": {
                    "@import.value": "$general.block_identifier",
                    "any_tag": ""
                },
                "speed": "$general.number"
            }
        }
    },
    "minecraft:fertilizer": {
        "type": "$item.general.fertilizer_type"
    },
    "minecraft:throwable": {
        "do_swing_animation": "$general.boolean",
        "max_draw_duration": "$general.number",
        "scale_power_by_draw_duration": "$general.boolean"
    },
    "minecraft:map": {
        "empty": "$general.boolean"
    },
    "minecraft:potion": {
        "type": "$item.general.potion_types"
    },
    "minecraft:hanging_entity": {
        "actor_type": "$general.number"
    },
    "minecraft:food": {
        "nutrition": "$general.number",
        "can_always_eat": "$general.boolean",
        "saturation_modifier": "$item.general.saturation_modifier",
        "effects": {
            "$dynamic.list.next_index": "$item.general.effect_definition"
        },
        "remove_effects": {
            "$dynamic.list.next_index": "$general.effect_name"
        },
        "using_converts_to": "$general.item_identifier",
        "on_use_action": ["chorus_teleport", "suspicious_stew_effect", "none"],
        "on_use_range": {
            "$dynamic.list.index_triple": "$general.number"
        }
    },
    "$dynamic_template.bridge:weapon_damage": {
        "$if": "$dynamic.bridge_core.is_active",

        "$default": "$general.number"
    },
    "$dynamic_template.bridge:item_equipped_sensor": {
        "$if": "$dynamic.bridge_core.is_active",

        "$default": {
            "on_equip": {
                "$dynamic.list.next_index": {
                    "@meta": {
                        "is_value": true
                    },
                    "@import.value": "$dynamic.cache.entity.events"
                }
            },
            "on_unequip": {
                "$dynamic.list.next_index": {
                    "@meta": {
                        "is_value": true
                    },
                    "@import.value": "$dynamic.cache.entity.events"
                }
            },
            "execute_commands": {
                "$dynamic.list.next_index": "$function.embedded"
            },

            "is_equipped": "$entity.components",
            "is_not_equipped": "$entity.components"
        }
    }
}