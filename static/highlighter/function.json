{
    "set": {
        "is_text_highlighter": true,
        "extension": "mcfunction",
        "line_comment": "#",
        "string_character": "\""
    },
    "define": {
        "keywords": [ 
            "minecraft", "alwaysday", "clear", "difficulty", "execute", "effect", "summon", "setblock", "fill", "scoreboard", "detect", "tellraw", "testfor", "testforblock", "testforblocks", "say", "tellraw", "kill", "setworldspawn", "spawnpoint", "gamemode", "gamerule", "tp", "teleport", "replaceitem", "clear", "enchant", "give", "weather", "xp", "clone", "title", "stopsound", "spreadplayers", "playsound", "tag", "help", "function", "locate", "tickingarea"
