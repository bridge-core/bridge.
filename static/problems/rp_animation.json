{
    "bridge:child_must_start_with": {
        "search": "animations",
        "start": "animation.",
        "error_message": "Animations must start with \"animation.<name>\"",
        "fix": {
            "type": "script",
            "run": "History('edit-key', node, node.key); node.key = `animation.${node.key}`;"
        }
    }
}